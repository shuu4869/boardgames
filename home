                    <script>
                        document.addEventListener('DOMContentLoaded', function () {
                            var imgModal = document.getElementById('image-modal');

                            imgModal.addEventListener('click', function () {
                                // สร้าง overlay
                                var overlay = document.createElement('div');
                                overlay.style.position = 'fixed';
                                overlay.style.top = 0;
                                overlay.style.left = 0;
                                overlay.style.width = '100%';
                                overlay.style.height = '100%';
                                overlay.style.backgroundColor = 'rgba(0, 0, 0, 0.7)';
                                overlay.style.zIndex = 9999;
                                overlay.style.display = 'flex';
                                overlay.style.justifyContent = 'center';
                                overlay.style.alignItems = 'center';

                                // สร้างโครงร่างภาพขยาย
                                var enlargedImgContainer = document.createElement('div');
                                enlargedImgContainer.style.textAlign = 'center';

                                // สร้างภาพขยาย
                                var enlargedImg = new Image();
                                enlargedImg.src = imgModal.src;
                                enlargedImg.width = '600';
                                enlargedImg.height = '600';
                                enlargedImg.style.borderRadius = '10px';
                                enlargedImg.style.boxShadow = '0 0 20px rgba(0, 0, 0, 0.5)';
                                enlargedImg.style.cursor = 'pointer';

                                // เพิ่มภาพขยายไปยังโครงร่าง
                                enlargedImgContainer.appendChild(enlargedImg);

                                // เพิ่มโครงร่างไปยัง overlay
                                overlay.appendChild(enlargedImgContainer);

                                // เพิ่ม overlay ไปยังเอกสาร
                                document.body.appendChild(overlay);

                                // ปิดโหมดขยายเต็มหน้าจอเมื่อคลิกที่ overlay
                                overlay.addEventListener('click', function (event) {
                                    if (event.target === overlay) {
                                        document.body.removeChild(overlay);
                                    }
                                });
                            });
                        });
                    </script>



        </main>


