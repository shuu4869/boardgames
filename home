                <div class=" alignwide  is-layout-flex wp-container-core-group-layout-6 wp-block-group-is-layout-flex"
                    style="margin-top:0;margin-bottom:0;display: flex; justify-content: center;">
                    <div class="container">
                        <div class="board-container">
                            <?php
                            // เชื่อมต่อกับฐานข้อมูล
                            $servername = "localhost";
                            $username = "root"; // ชื่อผู้ใช้ของคุณ
                            $password = "12345678"; // รหัสผ่านของคุณ
                            $dbname = "boardgame"; // ชื่อฐานข้อมูล
                            
                            // สร้างการเชื่อมต่อ
                            $conn = new mysqli($servername, $username, $password, $dbname);

                            // ตรวจสอบการเชื่อมต่อ
                            if ($conn->connect_error) {
                                die ("Connection failed: " . $conn->connect_error);
                            }

                            $gtotal = "SELECT COUNT(*) AS total_games FROM gamelist";

                            // คำสั่ง SQL เพื่อดึงข้อมูลเกมที่มีค่า view สูงสุด 5 อันดับ
                            $sql = "SELECT * FROM gamelist ORDER BY view DESC LIMIT 5";
                            $result = $conn->query($sql);

                            // ตรวจสอบว่ามีข้อมูลหรือไม่
                            if ($result->num_rows > 0) {
                                // แสดงข้อมูลในรูปแบบรายการ
                                while ($row = $result->fetch_assoc()) {
                                    echo "<a href='http://localhost/boardgame/gamedetails.php?id=" . $row["id"] . "' class='board-link' data-game-id='" . $row["id"] . "' onclick='updateView(" . $row["id"] . ")'>";
                                    echo "<div class='board' >";
                                    echo "<img src='data:image/jpeg;base64," . base64_encode($row["picture"]) . "' alt='Board Game Picture'>";
                                    echo "<div class='details'>";
                                    echo "<h2 style='font-size: 16px;'>" . $row["name"] . "</h2>";
                                    echo "<p><i class='fas fa-chess icon' style='color: #1e90ff;'></i> " . $row["type"] . "</p>";
                                    echo "<p><i class='fas fa-users icon' style='color: #32cd32;'></i>" . " " . $row["player"] . " Players" . "</p>";
                                    echo "<p><i class='fas fa-user icon' style='color: #ff6347;'></i>" . "  Age " . $row["age"] . " +" . "</p>";
                                    echo "<p><i class='fas fa-clock icon' style='color: #9370db;'></i>" . "  " . $row["time"] . " m" . "</p>";
                                    echo "<p><i class='fas fa-star icon' style='color: #ffcc00;'></i>" . "  " . $row["weight"] . "</p>";
                                    echo "</div>";
                                    echo "</div>";
                                    echo "</a>";

                                }
                            } else {
                                echo "<p>No games found.</p>";
                            }

                            // ปิดการเชื่อมต่อกับฐานข้อมูล
                            $conn->close();
                            ?>
                        </div>
                    </div>
                </div>
            </div>

